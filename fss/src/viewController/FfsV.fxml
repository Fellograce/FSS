<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="viewController.FfsC">
   <children>
      <VBox prefHeight="400.0" prefWidth="132.8">
         <children>
            <Button ellipsisString="" text="Herunterladen" VBox.vgrow="ALWAYS" fx:id="btDownload" onAction="#btDownloadOnAction">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button prefHeight="25.0" prefWidth="99.0" text="Hochladen" fx:id="btUpload" onAction="#btUploadOnAction">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <ListView prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" fx:id="lvFile">
         <HBox.margin>
            <Insets />
         </HBox.margin></ListView>
   </children>
</HBox>
