<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2A363F;" stylesheets="@fssStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewController.FSSC">
   <children>
      <VBox prefHeight="400.0" prefWidth="156.0">
         <children>
            <Button fx:id="btDownload" ellipsisString="" onAction="#btDownloadOnAction" text="Herunterladen" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btUpload" onAction="#btUploadOnAction" prefHeight="31.0" prefWidth="123.0" text="Hochladen">
               <VBox.margin>
                  <Insets left="20.0" right="20.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <ListView fx:id="lvFile" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #2A363F;" HBox.hgrow="ALWAYS">
         <HBox.margin>

         </HBox.margin></ListView>
   </children>
</HBox>